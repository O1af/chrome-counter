{
	"name": "Chrome-Counter",
	"description": "Counts the number of tabs a user has open in Chrome",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["storage"],
	"action": {
		"popup": "popup.html",
		"default_popup": "popup.html",
		"default_icon": "logo.png"
	}
}
